//TODO fix bug with timetable class (recreate)
$(function() {
    if ($('.timetable__content').children().hasClass('timetable')) { //change if way to .timetable changed
        $('.timetable').remove();
        render_timetable();
        $('.timetable__content').removeClass('d-none');
    }
    else {
        render_timetable();
        $('.timetable__content').removeClass('d-none');
    }
});

function render_timetable() {
    $('.timetable__content').append('<%= j render('timetable') %>')
}